#!/usr/bin/env php
<?php

namespace QL\Hal\Bin;

use MCP\Crypto\Utility\RandomUtility;

if (!$autoloader = @include __DIR__ . '/../vendor/autoload.php') {
    echo "An error occured while attempting to generate doctrine proxies.\n";
    exit(1);
}

$random = new RandomUtility;

echo strtolower($random->hex(64));
