{
    parser: 'babel-eslint',
    extends: 'eslint:recommended',

    plugins: [
        'react'
    ],

    globals: {
        document: true,
        window: true,
        module: true,
        console: true,
        '$': true,
        clearInterval: true,
        moment: true,
        require: true,
        React: true
    },
    parserOptions: {
        ecmaFeatures: {
          "jsx": true
        }
    },

    rules: {
        'no-console': 0
    }
}
