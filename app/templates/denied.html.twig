{% extends 'base.html.twig' %}

{% set page_title = "Access Denied" %}

{% block content %}
    <div class="error-page">
        {% set name = getUsersActualName(user) %}
        {% set msg = ["I'm sorry "~name~", I'm afraid I can't do that.", "Just what do you think you're doing, "~name~"?"] %}

        <h1>{{ random(msg) }}</h1>
        <img width="600px" src="/img/error/eye_large.jpg" />

    </div>
{% endblock %}
